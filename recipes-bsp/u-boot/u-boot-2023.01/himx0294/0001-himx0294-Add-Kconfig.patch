From 23764d9d3ab4074a2b539b60ca1fa08960ab2036 Mon Sep 17 00:00:00 2001
From: Mario Schuknecht <mario.schuknecht@dresearch-fe.de>
Date: Fri, 2 Jun 2023 05:48:14 +0000
Subject: [PATCH] himx0294: Add Kconfig

Add Kconfig support for himx0294 board.

Signed-off-by: Steffen Sledz <sledz@dresearch-fe.de>
Signed-off-by: Mario Schuknecht <mario.schuknecht@dresearch-fe.de>
---
 arch/arm/mach-imx/mx6/Kconfig | 6 ++++++
 1 file changed, 6 insertions(+)

diff --git a/arch/arm/mach-imx/mx6/Kconfig b/arch/arm/mach-imx/mx6/Kconfig
index 752c57f52d..d313df906c 100644
--- a/arch/arm/mach-imx/mx6/Kconfig
+++ b/arch/arm/mach-imx/mx6/Kconfig
@@ -508,6 +508,11 @@ config TARGET_NPI_IMX6ULL
 	select DM_THERMAL
 	select SUPPORT_SPL
 
+config TARGET_HIMX0294
+	bool "Support himx0294"
+	depends on MX6DL || MX6Q || MX6QDL || MX6S
+	select SUPPORT_SPL
+
 config TARGET_OPOS6ULDEV
 	bool "Armadeus OPOS6ULDev board"
 	depends on MX6UL
@@ -691,6 +696,7 @@ source "board/ge/b1x5v2/Kconfig"
 source "board/aristainetos/Kconfig"
 source "board/armadeus/opos6uldev/Kconfig"
 source "board/boundary/nitrogen6x/Kconfig"
+source "board/freescale/himx0294/Kconfig"
 source "board/bsh/imx6ulz_smm_m2/Kconfig"
 source "board/bticino/mamoj/Kconfig"
 source "board/compulab/cm_fx6/Kconfig"
-- 
2.34.1

